{
  "name": "sample",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.103",
    "@mui/material": "^5.10.9",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "17.0.2",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "mui-status": "^0.0.22",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.3.11",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.1",
    "typescript": "~4.8.4",
    "web-vitals": "^3.0.3"
  },
  "scripts": {
    "clean": "rm -rf node_modules && npm cache clean --force",
    "#postinstall": "npm run link.all",
    "link.all": "npm run link.react.dom && npm run link.react && npm run link.react.types && npm run link.react.dom.types && npm ls react && npm ls react-dom",
    "link.lib": "npm link mui-status && npm ls mui-status",
    "#MUI": "",
    "linkMuiCore": "cd node_modules/@mui/material && npm link",
    "linkMuiIcons": "cd node_modules/@mui/icons-material && npm link",
    "linkMuiStyles": "cd node_modules/@mui/styles && npm link",
    "linkMuiEngine": "cd node_modules/@mui/styled-engine && npm link",
    "#React": "react",
    "linkReact": "cd node_modules/react && npm link",
    "linkReactTypes": "cd node_modules/\\@types/react && npm link",
    "#ReactDom": "reactDom",
    "linkReactDom": "cd node_modules/react-dom && npm link",
    "linkReactDomTypes": "cd node_modules/\\@types/react-dom && npm link",
    "prestart": "npm run linkMuiCore && npm run linkMuiIcons && npm run linkReact && npm run linkReactDom && npm run link.lib",
    "start": "react-scripts start",
    "update": "npx ncu -u",
    "build": "react-scripts build",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.4.0"
  }
}
